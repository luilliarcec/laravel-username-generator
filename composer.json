{
    "name": "luilliarcec/laravel-username-generator",
    "description": "Generate user name from name, email or random.",
    "keywords": [
        "luilliarcec",
        "laravel-username-generator"
    ],
    "homepage": "https://github.com/luilliarcec/laravel-username-generator",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Luis Andr√©s Arce C.",
            "email": "luilliarcec@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Luilliarcec\\LaravelUsernameGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Luilliarcec\\LaravelUsernameGenerator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Luilliarcec\\LaravelUsernameGenerator\\LaravelUsernameGeneratorServiceProvider"
            ],
            "aliases": {
                "LaravelUsernameGenerator": "Luilliarcec\\LaravelUsernameGenerator\\LaravelUsernameGeneratorFacade"
            }
        }
    }
}
