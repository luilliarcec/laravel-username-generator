{
    "name": "luilliarcec/laravel-username-generator",
    "description": "Laravel Username Generator is a package that allows the versatile generation of user names, has a simple integration with Laravel.",
    "keywords": [
        "luilliarcec",
        "laravel-username-generator",
        "laravel",
        "username",
        "generator"
    ],
    "homepage": "https://github.com/luilliarcec/laravel-username-generator",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Luis Andr√©s Arce C.",
            "email": "luilliarcec@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2|^8.3",
        "laravel/framework": "^10.0|^11.0",
        "spatie/laravel-package-tools": "^1.14.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Luilliarcec\\LaravelUsernameGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Luilliarcec\\LaravelUsernameGenerator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Luilliarcec\\LaravelUsernameGenerator\\UsernameGeneratorServiceProvider"
            ],
            "aliases": {
                "Username": "Luilliarcec\\LaravelUsernameGenerator\\Facades\\Username"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
